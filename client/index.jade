  body
    .nav.navbar.navbar-default
      .container-fluid
        .navbar-header
          button.navbar-toggle.collapsed(data-toggle="collapse", data-target="#navbar")
            span.sr-only Toggle navigation
            span.icon-bar
            span.icon-bar
            span.icon-bar
        #navbar.collapse.navbar-collapse
          ul.nav.navbar-nav
            li: a(href='#portfolio')
                  i.fa.fa-laptop
                  |&nbsp; Portfolio 
            li: a(href='#experience')
                  i.fa.fa-briefcase
                  |&nbsp; Experience
            li: a(href='#blog')
                  i.fa.fa-newspaper-o
                  |&nbsp; Blog
            li: a(href='#about')
                  i.fa.fa-male 
                  |&nbsp; Me
    .container-fluid
      .row#head.text-center
        .col-xs-12
          h1 Tommy Janszen
          h4 Software Developer
          button.btn.btn-primary(data-toggle='modal' data-target='#contactModal') Contact
          
          #contactModal.modal.fade(tabindex='-1' role='dialog' aria-labelledby='contactModalLabel' aria-hidden='true')
            .modal-dialog
              .modal-content
                .modal-header
                  button.close(type='button' data-dismiss='modal' aria-hidden='true') x
                  h4#contactModalLabel.modal-title Contact Me
                .modal-body
                  .form
                    .form-group
                      .input-group
                        input#uName.form-control(type='text' placeholder='Name')
                        label.input-group-addon.glyphicon.glyphicon-user(for='uName' )
                    .form-group
                      .input-group
                        input#uSubject.form-control(type='text' placeholder='Subject')
                        label.input-group-addon.glyphicon.glyphicon-user(for='uSubject')
                .modal-footer
                  button.form-control.btn.btn-primary Send
                  .progress
                    .progress-bar.progress-bar-primary(role='progressbar' aria-valuenow='1' aria-valuemin='1' aria-valuemax='100' style='width:0%')
                      span.sr-only progress
      .row#skills
        .col-xs-6.col-sm-2.skill-img
          h5 html5
          img#html(src='img/skills/html.svg')
        .col-xs-6.col-sm-2.skill-img
          h5 css3
          img#css(src='img/skills/css3.svg')
        .col-xs-6.col-sm-2.skill-img
