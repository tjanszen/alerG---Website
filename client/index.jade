      //- Portfolio section
      .row#portfolio
        .col-xs-12.text-center 
          h3.sub-header-portfolio Portfolio
        .col-xs-12.col-sm-4
          .row
            .col-xs-offset-1.col-xs-10.portfolio-window#camp
              .col-xs-offset-1.col-xs-10.float-ups.portfolio-window-back-camp
                h4.float-ups-h4 Anamil Adoption Portal
                ul.list-inline
                  li Node.js + Hapi
                  li jQuery + Bootstrap
                  li CSS + LESS
                  li HTML5 + Jade
        .col-xs-12.col-sm-4
          .row
            .col-xs-offset-1.col-xs-10.portfolio-window#weather
              .col-xs-offset-1.col-xs-10.float-ups.portfolio-window-back-weather
                h4.float-ups-h4 Weather App
                ul.list-inline
                  li API (Underground Weather)
                  li jQuery + Bootstrap
                  li CSS + LESS
                  li HTML5 + Jade
              img(src='img/weather.png')
        .col-xs-12.col-sm-4
          .row
            .col-xs-offset-1.col-xs-10.portfolio-window#apartment
              .col-xs-offset-1.col-xs-10.float-ups.portfolio-window-back-apartment
